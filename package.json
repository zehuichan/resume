  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zehuichan/resume/issues"
  },
  "homepage": "https://github.com/zehuichan/resume#readme",
  "devDependencies": {
    "@vuepress/plugin-register-components": "^2.0.0-beta.48",
    "gh-pages": "^4.0.0",
    "release-it": "^15.1.0",
    "vuepress": "^2.0.0-beta.48"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "push": false,
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}"
    }
