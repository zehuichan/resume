    "release": "release-it && git push --follow-tags origin main",
    "gh-pages": "npm run docs:build && gh-pages -d docs/.vuepress/dist || gh-pages.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zehuichan/resume.git"
  },
  "keywords": [],
